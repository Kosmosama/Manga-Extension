{
    "manifest_version": 3,
    "name": "Manga Library",
    "description": "Bookmark for mangas/manhwas/manhuas extension",
    "permissions":["tabs","scripting","storage"],
    "version": "1.0",
    "action": {
      "default_popup": "src/index.html",
      "default_icon": "public/logos/icon.png"
    },
    "content_scripts":[{
      "matches":["<all_urls>"],
      "js":["/src/script.js"]
    }]
}